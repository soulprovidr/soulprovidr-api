{
  "globals": {
    "NODE_ENV": "test"
  },
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^@lib/(.*)$": "<rootDir>/../src/lib/$1"
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
